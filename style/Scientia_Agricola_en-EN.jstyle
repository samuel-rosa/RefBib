LAYOUT
article=\begin{author}\format[Authors(LastFirst,InitialsNospace,Semicolon,Semicolon), FormatChars, RemoveBrackets]{\author}\end{author} \year\uniq. \format[FormatChars]{\title}. \format[FormatChars]{\journal}\begin{volume} \format[FormatPagesForHTML]{\volume}\end{volume}\begin{pages}: \format[FormatPagesForHTML]{\pages}.\end{pages}\begin{pubstate} (\format[RemoveBrackets]{\pubstate}).\end{pubstate}\begin{note} \format[RemoveBrackets]{\note}.\end{note}\begin{doi} doi:\doi\end{doi}\begin{url} URL:\url\end{url}

book=\begin{author}\format[Authors(LastFirst,InitialsNospace,Semicolon, Semicolon), FormatChars, RemoveBrackets, AuthorAndsReplacer, NoSpaceBetweenAbbreviations]{\author}\end{author} \year\uniq. \format[FormatChars]{\title}.\begin{edition} \begin{edition} \edition.ed.\end{edition}\begin{publisher} \publisher,\end{publisher}\begin{address} \address\end{address}.\begin{language} \format[RemoveBrackets]{\language}\end{language}

collection=\begin{author}\format[Authors(LastFirst,InitialsNospace,Semicolon, Semicolon), FormatChars, AuthorAndsReplacer, NoSpaceBetweenAbbreviations]{\author}\end{author} \year\uniq. \format[FormatChars]{\title}.\begin{pages} p.\format[FormatPagesForHTML]{\pages}\end{pages}\begin{edition} \edition.ed.\end{edition}\begin{publisher} \publisher,\end{publisher}\begin{address} \address\end{address}.

conference=\begin{author}\format[Authors(LastFirst,InitialsNospace,Semicolon, Semicolon), FormatChars, AuthorAndsReplacer, NoSpaceBetweenAbbreviations]{\author}\end{author} \format[FormatChars]{\title}. In: \begin{booktitle}{\booktitle},\end{booktitle}\begin{edition} \edition},\end{edition}\begin{address} \address,\end{address}\begin{year} \year\end{year}. Anais.\begin{publisher} \publisher,\end{publisher}\begin{address} \address\end{address} \year\uniq.\begin{pages} p.\format[FormatPagesForHTML]{\pages}\end{pages}.\begin{howpublished} (\howpublished)\end{howpublished}

default=\begin{author}\format[Authors(LastFirst,InitialsNospace,Semicolon, Semicolon), FormatChars, AuthorAndsReplacer, NoSpaceBetweenAbbreviations]{\author}\end{author} \year\uniq. \format[FormatChars]{\title}.\begin{publisher} \publisher,\end{publisher}\begin{address} \address\end{address}.\begin{pages} \format[FormatPagesForHTML]{\pages}p.\end{pages}

electronic=\begin{author}\format[Authors(LastFirst,InitialsNospace,Semicolon, Semicolon), FormatChars, AuthorAndsReplacer, NoSpaceBetweenAbbreviations]{\author}\end{author} \format[FormatChars]{\title}. Available in <\url>. Accessed in \month, \year\uniq.\begin{howpublished} (\howpublished)\end{howpublished}

inbook=\begin{author}\format[Authors(LastFirst,InitialsNospace,Semicolon, Semicolon), FormatChars, AuthorAndsReplacer, NoSpaceBetweenAbbreviations]{\author}\end{author} \year\uniq. \format[FormatChars]{\title}.\begin{pages} p. \format[FormatPagesForHTML]{\pages}.\end{pages} In: \begin{editor}\format[Authors(LastFirst,InitialsNospace,Semicolon, Semicolon), FormatChars, RemoveBrackets]{\editor}, \format[IfPlural(eds.,ed.)]{\editor}\end{editor}\begin{booktitle} \format[FormatChars, RemoveBrackets]{\booktitle}.\end{booktitle}\begin{edition} \edition.ed.\end{edition}\begin{publisher} \publisher,\end{publisher}\begin{address} \format[FormatChars, RemoveBrackets]{\address}\end{address}.

incollection=\begin{author}\format[Authors(LastFirst,InitialsNospace,Semicolon, Semicolon), FormatChars]{\author}\end{author} \year\uniq. \format[FormatChars]{\title}. In: \begin{editor}\format[Authors(LastFirst,NoPunc,InitialsNospace,Semicolon, Semicolon)]{\editor} (\format[IfPlural(eds.,ed.)]{\editor}). \end{editor}\begin{booktitle} \booktitle.\end{booktitle}\begin{edition} \edition.ed.\end{edition}\begin{publisher} \publisher,\end{publisher}\begin{address} \address,\end{address}.\begin{pages} \format[FormatPagesForHTML]{\pages}p.\end{pages}

inproceedings=\begin{author}\format[Authors(LastFirst,InitialsNospace,Semicolon, Semicolon), FormatChars]{\author}\end{author} \year\uniq. \format[FormatChars]{\title}. In: \begin{booktitle}\booktitle\end{booktitle}; \begin{eventdate}\eventdate; \end{eventdate}\begin{location}\location\end{location}.\begin{publisher} \publisher,\end{publisher}\begin{address} \address.\end{address}\begin{pages} p.\format[FormatPagesForHTML]{\pages}\end{pages}.

manual=\begin{author}\format[Authors(LastFirst,InitialsNospace,Semicolon, Semicolon), FormatChars]{\author}\end{author} \year\uniq. \format[FormatChars]{\title}.\begin{edition} \edition.ed.\end{edition}\begin{publisher} \publisher,\end{publisher}\begin{address} \address,\end{address}.

