monograph=\begin{author}\format[Authors(LastFirst,InitialsNospace), FormatChars]{\author}\end{author} \year\uniq. \format[FormatChars]{\title} [Monograph]. \begin{address}\address\end{address}: \begin{school}\school\end{school}.

phdthesis=\begin{author}\format[Authors(LastFirst,InitialsNospace), FormatChars]{\author}\end{author} \year\uniq. \format[FormatChars]{\title} [Doctorate Thesis]. \begin{address}\address\end{address}: \begin{school}\school\end{school}.

techreport=\begin{author}\format[Authors(LastFirst,InitialsNospace,Semicolon, Semicolon), FormatChars]{\author}\end{author} \year\uniq. \format[FormatChars]{\title}.\begin{edition} \edition.ed.\end{edition}\begin{address} \address,\end{address}\begin{publisher} \format[RemoveBrackets, FormatChars]{\publisher}\end{publisher}\begin{institution} \format[RemoveBrackets, FormatChars]{\institution}\end{institution}.\begin{pages} \format[FormatPagesForHTML]{\pages}p.\end{pages}

unpublished=\begin{author}\format[Authors(LastFirst,InitialsNospace,Semicolon, Semicolon), FormatChars]{\author}\end{author} \year\uniq. \format[FormatChars]{\title}.\begin{edition} \edition.ed.\end{edition}\begin{address} \address,\end{address}\begin{publisher} \publisher,\end{publisher}.\begin{pages} \format[FormatPagesForHTML]{\pages}p.\end{pages}
