LAYOUT
article=\begin{author}\format[Authors(LastFirst, sep=; ), AuthorAndsReplacer, NoSpaceBetweenAbbreviations, ToUpperCase]{\author}\end{author} \title. \format[JournalAbbreviator]{\journal}, \volume:\begin{pages}\format[FormatPagesForHTML]{\pages}\end{pages}, \year\uniq.

book=\begin{author}\format[Authors(LastFirst, sep=; ), AuthorAndsReplacer, NoSpaceBetweenAbbreviations, ToUpperCase]{\author}\end{author} \title.\begin{edition} \edition.ed.\end{edition}\begin{address} \address,\end{address}\begin{publisher} \publisher,\end{publisher} \year\uniq.\begin{pages} \format[FormatPagesForHTML]{\pages}p.\end{pages}

collection=\begin{author}\format[Authors(LastFirst, sep=; ), AuthorAndsReplacer, NoSpaceBetweenAbbreviations, ToUpperCase]{\author}\end{author} \title.\begin{edition} \edition.ed.\end{edition}\begin{address} \address,\end{address}\begin{publisher} \publisher,\end{publisher} \year\uniq.\begin{pages} \format[FormatPagesForHTML]{\pages}p.\end{pages}

conference=\begin{author}\format[Authors(LastFirst, sep=; ), AuthorAndsReplacer, NoSpaceBetweenAbbreviations, ToUpperCase]{\author}\end{author} \title. In:\begin{booktitle} \format[ToUpperCase]{\booktitle},\end{booktitle}\begin{edition} \edition},\end{edition}\begin{address} \address,\end{address}\begin{year} \year\end{year}. Anais.\begin{address} \address,\end{address}\begin{publisher} \publisher,\end{publisher} \year\uniq.\begin{pages} p.\format[FormatPagesForHTML]{\pages}\end{pages}.\begin{howpublished} (\howpublished)\end{howpublished}

default=\begin{author}\format[Authors(LastFirst, sep=; ), AuthorAndsReplacer, NoSpaceBetweenAbbreviations, ToUpperCase]{\author}\end{author} \title.\begin{address} \address,\end{address}\begin{publisher} \publisher,\end{publisher} \year\uniq.\begin{pages} \format[FormatPagesForHTML]{\pages}p.\end{pages}

electronic=\begin{author}\format[Authors(LastFirst, sep=; ), AuthorAndsReplacer, NoSpaceBetweenAbbreviations, ToUpperCase]{\author}\end{author} \title. Disponível em <\url>. Acessado em \month de \year\uniq.\begin{howpublished} (\howpublished)\end{howpublished}

inbook=\begin{author}\format[Authors(LastFirst, sep=; ), AuthorAndsReplacer, NoSpaceBetweenAbbreviations, ToUpperCase]{\author}\end{author} \title. In: \begin{editor}\format[Authors(LastFirst, sep=; ), AuthorAndsReplacer, NoSpaceBetweenAbbreviations, ToUpperCase]{\editor} (\format[IfPlural(eds.,ed.)]{\editor}). \end{editor}\begin{booktitle} \booktitle.\end{booktitle}\begin{edition} \edition.ed.\end{edition}\begin{address} \address,\end{address}\begin{publisher} \publisher,\end{publisher} \year\uniq.\begin{pages} \format[FormatPagesForHTML]{\pages}p.\end{pages}

incollection=\begin{author}\format[Authors(LastFirst, sep=; ), AuthorAndsReplacer, NoSpaceBetweenAbbreviations, ToUpperCase]{\author}\end{author} \title. In: \begin{editor}\format[Authors(LastFirst, sep=; ), AuthorAndsReplacer, NoSpaceBetweenAbbreviations, ToUpperCase]{\editor} (\format[IfPlural(eds.,ed.)]{\editor}). \end{editor}\begin{booktitle} \booktitle.\end{booktitle}\begin{edition} \edition.ed.\end{edition}\begin{address} \address,\end{address}\begin{publisher} \publisher,\end{publisher} \year\uniq.\begin{pages} \format[FormatPagesForHTML]{\pages}p.\end{pages}

inproceedings=\begin{author}\format[Authors(LastFirst, sep=; ), AuthorAndsReplacer, NoSpaceBetweenAbbreviations, ToUpperCase]{\author}\end{author}  \title. In: \begin{booktitle}\format[ToUpperCase]{\booktitle},\end{booktitle}\begin{edition} \edition},\end{edition}\begin{address} \address,\end{address} \year\uniq. Anais.\begin{address} \address,\end{address}\begin{publisher} \publisher,\end{publisher} \year\uniq.\begin{pages} p.\format[FormatPagesForHTML]{\pages}\end{pages}.\begin{howpublished} (\howpublished)\end{howpublished}

